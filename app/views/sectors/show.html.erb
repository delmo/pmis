<% @page_title = "Sector" %>
<div id="pads"> 
 <%= link_to("Back to list", {:action => "index"}, :class => "btn btn-primary") %>
</div>

<div class="panel panel-default">
 <div class="panel-heading">
  Show Sector
 </div>

 <table class="table" summary="sector detail view">
  <tr>
   <th>Name</th>
   <td><%= @sector.name %></td>
  </tr>
  <tr>
   <th>Description</th>
   <td><%= @sector.description %></td>
  </tr>
  <tr>
   <th>Goal</th>
   <td><%= @sector.goal %></td>
  </tr>
  <tr>
   <th>Development Thrust</th>
   <td><%= @sector.development_thrust %></td>
  </tr>
  <tr>
   <th>Objective</th>
   <td><%= @sector.objective %></td>
  </tr>
  <tr>
   <th>Desired Outcome</th>
   <td><%= @sector.desired_outcome %></td>
  </tr>
  <tr>
   <th>Required Policy</th>
   <td><%= @sector.required_policy %></td>
  </tr>
 </table>
</div>

