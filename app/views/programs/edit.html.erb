<% @page_title = "Edit Program" %>
<div> 
 <%= link_to("Back to list", {:action => "index"}, :class => "btn btn-primary") %>
</div>

<div class="program edit">
 <h2>Edit Program</h2>

 <%= form_for(@program, :html => {:role => "form"}) do |f| %>

  <%= render(:partial => 'form', :locals => {:f => f}) %>

  <div class="form-group">
   <div class="col-sm-2"></div>
   <div class="col-sm-10">
   <%= submit_tag("Update Program", :class => "btn btn-default") %>
  </div>
  </div>

 <% end%>
</div>
